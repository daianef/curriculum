\cvsection{<%= @experience_title %>}
\begin{cventries}
  <%= @experiences.each do |exp| %>
    \cventry
      {<%= exp['company'] %>}
      {<%= exp['designation'] %>}
      {}
      {<%= exp['start_date'] %> - <%= exp['end_date'] %>}
      {
        \begin{cvitems}
          \item {<%= exp['description'] %>}
        \end{cvitems}
      }
  <% end %>
\end{cventries}
