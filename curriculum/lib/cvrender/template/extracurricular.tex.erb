\cvsection{<%= @structure.get_section_title_by_file(__FILE__) %>}
\begin{cventries}
  <% @extracurriculars.to_a.each do |ext| %>
    \cventry
      {<%= ext['title'] %>}
      {<%= ext['location'] %>}
      {<%= ext['date'] %>}
      {
        \begin{cvitems}
          \item {<%= ext['description'] %>}
        \end{cvitems}
      }
  <% end %>
\end{cventries}
