\cvsection{<%= @structure.get_section_title_by_file(__FILE__) %>}
\begin{cventries}
  <% @educations.each do |edu| %>
    \cventry
      {<%= edu['degree'] %>}
      {<%= edu['institute'] %>}
      {}
      {<%= edu['start_date'] %> - <%= edu['end_date'] %>}
      {
        \begin{cvitems}
          <% edu['descriptions'].each do |desc| %>
            \item {<%= desc %>}
          <% end %>
        \end{cvitems}
      }
  <% end %>
\end{cventries}
